{
  "name": "bazaar-vid",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fonts:sync": "tsx scripts/sync-fonts-from-r2.ts",
    "fonts:sync:prune": "tsx scripts/sync-fonts-from-r2.ts --prune",
    "build": "npm run generate:types && next build",
    "copy:fonts": "node scripts/copy-fonts.js && node scripts/vendor-fonts.cjs",
    "build:task-processor": "tsc --project tsconfig.task-processor.json",
    "build:log-agent": "cd src/scripts/log-agent && npm run build",
    "check": "next lint && tsc --noEmit",
    "check:export": "node scripts/check-export-config.js",
    "db:generate": "tsx node_modules/.bin/drizzle-kit generate",
    "db:migrate": "tsx node_modules/.bin/drizzle-kit migrate",
    "db:push": "tsx node_modules/.bin/drizzle-kit push",
    "db:studio": "tsx node_modules/.bin/drizzle-kit studio",
    "db:seed": "tsx scripts/dev-seed.ts",
    "db:push-neon": "tsx scripts/migrate-neon.ts",
    "db:update-credits": "dotenv -e .env.local -- tsx src/scripts/update-credit-system.ts",
    "generate:types": "tsx scripts/generate-types.ts",
    "setup:s3-public": "node scripts/setup-s3-public-access.js",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' npm run generate:types && next dev",
    "dev:verbose": "TRPC_DEBUG=true next dev",
    "dev:quiet": "next dev",
    "dev:ultra-quiet": "ULTRA_QUIET=true next dev",
    "dev:clean": "tsx scripts/filtered-dev.ts",
    "dev:silence": "ABSOLUTE_SILENCE=true next dev",
    "dev:no-restart": "WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=true next dev",
    "dev:stable": "NEXT_MANUAL_SIG_HANDLE=true next dev",
    "evals": "dotenv -e .env.local -- tsx --require tsconfig-paths/register scripts/run-evals.ts",
    "evals:quick": "npm run evals quick",
    "evals:compare": "npm run evals compare",
    "eval": "dotenv -e .env.local -- tsx --require tsconfig-paths/register scripts/run-performance-evals.ts",
    "eval:compare": "npm run eval compare-models",
    "eval:quick": "npm run eval quick-test",
    "eval:benchmark": "npm run eval benchmark",
    "eval:pipeline": "npm run eval test-pipeline",
    "eval:prompts": "npm run eval optimize-prompts",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "fresh": "rm -rf .next node_modules package-lock.json && npm cache clean && npm install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "remotion:deploy": "node scripts/deploy-remotion-site.js",
    "start": "next start",
    "test": "NODE_ENV=development dotenv -e .env.local -- node node_modules/jest/bin/jest.js",
    "test:watch": "NODE_ENV=development dotenv -e .env.local -- node node_modules/jest/bin/jest.js --watch",
    "test:phase1": "NODE_ENV=development dotenv -e .env.local -- node node_modules/jest/bin/jest.js \"(wrapSceneNamespace\\.phase1|scene-compiler\\.phase1|composite-header\\.phase1)\\.test\\.ts$\"",
    "test:screenshots": "dotenv -e .env.local -- tsx src/tools/webAnalysis/__tests__/screenshot-test.ts",
    "test:template-selection": "tsx src/tests/run-intelligent-tests.ts unit",
    "test:e2e": "tsx src/tests/run-intelligent-tests.ts e2e",
    "test:all": "tsx src/tests/run-intelligent-tests.ts all",
    "test:fixtures": "tsx src/tests/run-intelligent-tests.ts fixtures",
    "test:streaming": "NODE_ENV=development dotenv -e .env.local -- jest src/tests/integration/streaming-pipeline.test.ts",
    "test:streaming:edge": "NODE_ENV=development dotenv -e .env.local -- jest src/tests/integration/streaming-edge-cases.test.ts",
    "test:sse": "NODE_ENV=development dotenv -e .env.local -- jest src/tests/integration/sse-streaming.test.ts",
    "test:streaming:all": "NODE_ENV=development dotenv -e .env.local -- jest src/tests/integration/streaming*.test.ts src/tests/integration/sse*.test.ts",
    "test:ci": "NODE_ENV=development dotenv -e .env.local -- node node_modules/jest/bin/jest.js --ci --coverage",
    "typecheck": "tsc --noEmit",
    "templates:verify": "tsx scripts/verify-templates.ts",
    "debug:media-plan": "dotenv -e .env.local -- tsx scripts/debug-media-plan.ts",
    "debug:media-plan-suite": "dotenv -e .env.local -- tsx scripts/run-media-plan-suite.ts",
    "log:agent": "npm run log-agent",
    "log:dev": "cd src/scripts/log-agent && npm run dev",
    "log:clear": "npm run build:log-agent && cd src/scripts/log-agent && node dist/cli.js clear",
    "log:ask": "npm run build:log-agent && cd src/scripts/log-agent && node dist/cli.js ask",
    "log:logs": "npm run build:log-agent && cd src/scripts/log-agent && node dist/cli.js logs",
    "log:issues": "npm run build:log-agent && cd src/scripts/log-agent && node dist/cli.js issues",
    "log:interactive": "npm run build:log-agent && cd src/scripts/log-agent && node dist/cli.js interactive",
    "log-agent": "npm run build:log-agent && cd src/scripts/log-agent && npm run start",
    "log:ask:mcp": "tsx src/scripts/log-tools/log-ask.ts",
    "log:refresh:mcp": "tsx src/scripts/log-tools/log-refresh.ts",
    "log:issues:mcp": "tsx src/scripts/log-tools/log-issues.ts",
    "log:raw:mcp": "tsx src/scripts/log-tools/log-raw.ts",
    "analyze:errors": "tsx scripts/analyze-production-errors.ts",
    "postinstall": "npm rebuild lightningcss @tailwindcss/oxide || true && npx playwright install chromium || true",
    "sanitize:avatars": "tsx scripts/sanitize-scenes-avatars.ts"
  },
