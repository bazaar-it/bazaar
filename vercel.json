{
  "functions": {
    "src/app/api/generate-stream/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/upload/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "/tmp/ms-playwright"
  },
  "crons": [
    {
      "path": "/api/cron/daily-reset",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/data-lifecycle-cleanup",
      "schedule": "0 3 * * *"
    }
  ]
}